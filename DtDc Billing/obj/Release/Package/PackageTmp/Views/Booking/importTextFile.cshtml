
@{
    ViewBag.Title = "importTextFile";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div class="content-wrapper">
    <div class="box box-info">

        <div class="box-header with-border">
            <h4>Import From Text</h4>
        </div><!-- /.box-header -->
        <div class="box-body">
            <div class="row">
                <div class="col-md-12">

                    <div class="form-horizontal">

                        <div class="row">
                            <div class="col-md-4">
                                <div class="form-group">
                                    @using (Html.BeginForm("importTextFile", "Booking", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                    {
                                        <label class="">Upload Text or Csv  File </label>
                                        <div class="">
                                            <input id="File1" type="file" name="ImportText" />
                                        </div>
                                        <div class="">
                                            <input type="submit" class="btn btn-success mt-3" value="Upload" style="margin-top:20px" />
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
  
