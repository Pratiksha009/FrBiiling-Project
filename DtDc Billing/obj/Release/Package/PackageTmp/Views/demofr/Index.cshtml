@model IEnumerable<DtDc_Billing.Entity_FR.Franchisee>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<section>
    <div class="container">
        <h2>Index</h2>

        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.F_Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.F_Address)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.OwnerName)
