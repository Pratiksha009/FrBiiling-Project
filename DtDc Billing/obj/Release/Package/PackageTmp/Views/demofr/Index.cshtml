                <th>
                    @Html.DisplayNameFor(model => model.Branch_Area)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Datetime_Fr)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Pan_No)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FirmDetail.Firm_Name)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.F_Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.F_Address)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.OwnerName)
