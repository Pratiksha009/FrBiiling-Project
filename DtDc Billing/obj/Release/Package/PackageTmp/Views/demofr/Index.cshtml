                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Branch_Area)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Datetime_Fr)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Pan_No)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FirmDetail.Firm_Name)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.PF_Code }) |
                        @Html.ActionLink("Details", "Details", new { id = item.PF_Code }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.PF_Code })
                    </td>
                </tr>
            }

        </table>
    </div>
</section>
