            select: function (event, ui) {

            }
        });
        });



     $('#datepicker').datepicker({
         autoclose: true,
         format: 'dd-mm-yyyy'
     })

     $('#datepicker1').datepicker({
         autoclose: true,
         format: 'dd-mm-yyyy'
     })

    });
